{
  "name": "@cspell/dict-common-code-compounds",
  "version": "1.0.0",
  "description": "Common Code Compound Words dictionary for cspell. -- Private until verified",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 cspell-tools-cli compile \"src/common-words.txt\" -o .",
    "test": "head -n 1000 \"src/common-words.txt\" | cspell -v -c ./cspell-ext.json \"--local=*\" \"--languageId=*\" stdin",
    "prepublishOnly": "yarn test",
    "prepare": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "common-code-compounds",
    "Common Code Compound Words",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/packages/common-code-compounds#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "cpp.txt.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
